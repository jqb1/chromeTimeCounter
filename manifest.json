{
  "manifest_version": 2,

  "name": "Website timer",
  "description": "This extension counts time you spent on a different websites",
  "version": "1.0",

  "content_scripts":[{
    "matches":["<all_urls>"],
    "js":["timer.js"]
  }],

  "browser_action": {
    "default_icon": "icon.png"

  },

  "background": {
    "scripts":["background.js"]
  },
  "permissions": [
    "tabs",
    "cookies",
    "<all_urls>",
    "storage"
  ]




}